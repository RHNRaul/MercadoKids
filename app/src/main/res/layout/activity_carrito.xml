<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"

        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:background = "@color/white"
        android:layout_height="match_parent"
        tools:context=".Carrito">

    <Button
            android:text="Cerrar Sesion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button5"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="161dp"
            android:layout_marginEnd="162dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="19dp"
            app:layout_constraintTop_toBottomOf="@+id/RV2" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="64dp"/>
    <androidx.recyclerview.widget.RecyclerView
            android:layout_width="0dp"
            android:layout_height="600dp" android:id="@+id/RV2" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="132dp" app:layout_constraintTop_toTopOf="parent">
    </androidx.recyclerview.widget.RecyclerView>

</androidx.constraintlayout.widget.ConstraintLayout>